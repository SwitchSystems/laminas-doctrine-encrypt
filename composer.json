{
    "name": "somecoding/laminas-doctrine-encrypt",
    "description": "Provides property encryption and hashing for Doctrine Entities when used within Laminas",
    "keywords": [
        "zf3",
        "doctrine2",
        "encrypt",
        "module",
        "zend framework 3",
        "laminas",
        "encryption"
    ],
    "homepage": "https://keet.me/",
    "license": "MIT",
    "authors": [
        {
            "name": "Dwarfex",
            "email": "dwarfex@somecoding.de",
            "homepage": "https://somecoding.de"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-sodium": "*",
        "doctrine/annotations": "^1.10",
        "doctrine/orm": "^2.14",
        "doctrine/doctrine-module": "^5.0",
	"paragonie/halite": "^4.7",
        "paragonie/hidden-string": "^1.1",
        "laminas/laminas-modulemanager": "^2.8",
        "laminas/laminas-servicemanager": "^3.6",
        "laminas/laminas-stdlib": "^3.2",
        "laminas/laminas-dependency-plugin": "^2.1"
    },
    "extra": {
      "branch-alias": {
      	"dev-php81": "3.1.x-dev"
      }
    },
    "autoload": {
        "psr-4": {
            "Keet\\Encrypt\\": "src/"
        },
        "classmap": [
            "src/Module.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true
        }
    }
}
